
public class Car 
{
	
	public Car () //sets the mpg equal to miles/gallon
	{
		
		gallon = 0;
		
	}
	
	public Car (double mpg)
	{
		
		mpg = miles/gallon;
		
	}
	
	public double drive (int travel)
	{
		
		gallons = gallons - gallons/miles * travel;
		
	}
	
	public double addGas(int dasdasdasd)
	{
		
		gallons = gallons + dasdasdasd;
		
	}
	
	public double getGas;
	{
		return gallons;
	}
	
	private int miles;
	
	private double gallon;
	
}
